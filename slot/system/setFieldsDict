FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue F0  0.5
);

regions
(
    boxToCell
    {
        box (0 -0.15625 -0.5) (12 0.15625 0.5);
        fieldValues
        (
            volScalarFieldValue F0 0.0
        );
    }
);

